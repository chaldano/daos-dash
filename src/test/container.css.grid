    display: flex;
    /* height: 300px; */
    flex-wrap: wrap;
    align-items: flex-start;
    justify-content: center;
    flex-direction: row;
background-color: gray ;
}

navb1 {

  display: flex;
  background-color: black;
  padding: 10px 0;

}

navb1 a {
  flex: auto;
  text-align: center;
  background-color: #ccc;
  color: black;
  margin: 0 5px;
  padding: 5px 0;
  text-decoration: none;
}

navb1 a:hover {
  outline: 1px solid red;
  color: red;
  text-decoration: underline;
}
navb1 button {
  flex: auto;
  text-align: center;
  background-color: #ccc;
  color: black;
  margin: 0 5px;
  padding: 5px 0;
  text-decoration: none;
}

navb1 button:hover {
  outline: 1px solid red;
  color: red;
  text-decoration: underline;
}

