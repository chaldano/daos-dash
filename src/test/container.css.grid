p.flex-item{
    font-weight: bold;
    text-align: center;
}
.grid {
    stroke: #9A8B7A;
    stroke-width: 1px;
    fill: #CF7D1C;
  }
  .arc {
    stroke: #9A8B7A;
    fill: none;
  }
  .node {
    fill: #EBD8C1;
    stroke: #9A8B7A;
    stroke-width: 1px;
  }
  circle.active {
    fill: #FE9922;
  }
  path.active {
    stroke: #FE9922;
  }
  circle.source {
    fill: #93C464;
  }
  circle.target {
    fill: #41A368;
  }
  #vis {
    border: 1px solid;
  }